{
  "expo": {
    "name": "MyExpoTSProject",
    "slug": "MyExpoTSProject",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "userInterfaceStyle": "automatic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./App/assets/images/EscherCube.png",
    "splash": {
      "image": "./App/assets/images/EscherCube.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "packagerOpts": {
      "config": "metro-config.js"
    }
  }
}
